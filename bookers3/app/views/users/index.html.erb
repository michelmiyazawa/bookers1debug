<%= render 'users/list', user: @user, book: @book %>
<div class='col-xs-9'>
<h2>Users</h2>
<table class='table'>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>
  <% @users.each do |user| %>
  <tbody>
    <tr>
      <td><%= attachment_image_tag user, :profile_image, size: "50x50", format: 'jpg', fallback: "no_image.jpg" %></td>
      <td><%= user.name %></td>
      <td><%= link_to "Show", user %></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>